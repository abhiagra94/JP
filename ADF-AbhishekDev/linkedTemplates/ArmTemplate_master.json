{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "ADF-AbhishekDev"
		},
		"BlobLS_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'BlobLS'"
		},
		"Gen2IR_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'Gen2IR'"
		},
		"sqldatbase_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'sqldatbase'",
			"defaultValue": "Integrated Security=False;Encrypt=True;Connection Timeout=30;Data Source=sqlserver123abhi.database.windows.net;Initial Catalog=sqldatabase123;User ID=Cloud"
		},
		"Gen2IR_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://storagegen2link.dfs.core.windows.net/"
		},
		"keyvaultxj_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://keyvaultxj.vault.azure.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "ADF-AbhishekDev_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"BlobLS_connectionString": {
						"value": "[parameters('BlobLS_connectionString')]"
					},
					"Gen2IR_accountKey": {
						"value": "[parameters('Gen2IR_accountKey')]"
					},
					"sqldatbase_connectionString": {
						"value": "[parameters('sqldatbase_connectionString')]"
					},
					"Gen2IR_properties_typeProperties_url": {
						"value": "[parameters('Gen2IR_properties_typeProperties_url')]"
					},
					"keyvaultxj_properties_typeProperties_baseUrl": {
						"value": "[parameters('keyvaultxj_properties_typeProperties_baseUrl')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "ADF-AbhishekDev_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'ADF-AbhishekDev_ArmTemplate_0')]"
			]
		}
	]
}